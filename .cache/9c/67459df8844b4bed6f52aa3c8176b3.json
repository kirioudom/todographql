mn":1561}},{"source":"schema/schema.ts","name":null,"original":{"line":132,"column":9},"generated":{"line":1,"column":1572}}],"sources":{"schema/schema.ts":"import {\n  GraphQLObjectType,\n  GraphQLString,\n  GraphQLBoolean,\n  GraphQLSchema,\n  GraphQLList\n} from \"graphql\";\n\nlet todoMocking = [\n  {\n    id: \"1\",\n    task: \"Cleaning\",\n    done: false\n  },\n  {\n    id: \"2\",\n    task: \"Writing\",\n    done: false\n  },\n  {\n    id: \"3\",\n    task: \"Reading\",\n    done: false\n  }\n];\n\nconst TodoType = new GraphQLObjectType({\n  name: \"Todo\",\n  fields: () => ({\n    id: { type: GraphQLString },\n    task: { type: GraphQLString },\n    done: { type: GraphQLBoolean }\n  })\n});\n\nconst queryType = new GraphQLObjectType({\n  name: \"Query\",\n  fields: () => ({\n    allTodos: {\n      type: new GraphQLList(TodoType),\n      resolve() {\n        return todoMocking;\n      }\n    },\n    todo: {\n      type: TodoType,\n      args: { id: { type: GraphQLString } },\n      resolve(parentValue, args, context, info) {\n        return todoMocking.find(todo => todo.id === args.id);\n      }\n    }\n  })\n});\n\ninterface Todo {\n  id: string;\n  task: string;\n  done: boolean;\n}\n\nfunction getAllID(listID: Todo[]): string[] {\n  return listID.map(({ id }) => id);\n}\n\nfunction isIDexist(generatedID: string, listID: string[]) {\n  const hasID = listID.find(idTodo => idTodo === generatedID);\n  return hasID ? true : false;\n}\n\nfunction generateID() {\n  const id: string = `${Math.ceil(Math.random() * 100)}`;\n  return id;\n}\nfunction generateUniqueID(): string {\n  let generatedID = generateID();\n  const allTodosID = getAllID(todoMocking);\n  while (isIDexist(generatedID, allTodosID)) {\n    generatedID = generateID();\n  }\n  return generatedID;\n}\n\nfunction findIDAndDelete(id: string, todos: Todo[]) {\n  return todos.filter(todo => todo.id !== id);\n}\n\nconst DeletedResponseType = new GraphQLObjectType({\n  name: \"DeletedResponse\",\n  fields: () => ({\n    message: { type: GraphQLString },\n    isDelete: { type: GraphQLBoolean }\n  })\n});\n\nconst rootMutation = new GraphQLObjectType({\n  name: \"Mutation\",\n  fields: {\n    addTodo: {\n      type: TodoType,\n      args: {\n        task: { type: GraphQLString },\n        done: { type: GraphQLBoolean }\n      },\n      resolve(parentValue, { task, done }) {\n        const generatedID = generateUniqueID();\n        const newTodo: Todo = { id: generatedID, task, done };\n        todoMocking.push(newTodo);\n        return newTodo;\n      }\n    },\n    removeTodo: {\n      type: DeletedResponseType,\n      args: { id: { type: GraphQLString } },\n      resolve(parentValue, { id }) {\n        const isIDExist = todoMocking.find(todo => todo.id === id);\n        if (isIDExist === undefined) {\n          return { message: \"TODO ID does not exists!\", isDelete: false };\n        }\n        const newTodo = findIDAndDelete(id, todoMocking);\n        todoMocking = newTodo;\n        return { message: \"Delete Successed\", isDelete: true };\n      }\n    }\n  }\n});\n\nconst rootSchema = new GraphQLSchema({\n  query: queryType,\n  mutation: rootMutation\n});\n\nexport { rootSchema };\n"},"lineCount":null}},"error":null,"hash":"023f29acec421f11f88f8236f9312d2d","cacheData":{"env":{}}}